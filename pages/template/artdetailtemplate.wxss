@import "../../utils/face.wxss";

/*标签容器样式*/

.container-tags {
  margin-top: 20rpx;
  margin-left: 26rpx;
  margin-right: 26rpx;
}

/*标签的文字样式*/

.text-tags-style {
  font-size: 30rpx;
}

/*点赞容器样式*/

.container-like {
  height: 140rpx;
  width: 140rpx;
  margin: 0 auto;
  background: #eee;
  border-radius: 50%;
  overflow: visible;
  text-align: center;
  margin-top: 22rpx;
  position: relative;
}

.btn-login-zan-conetnt {
  height: 140rpx;
  width: 140rpx;
  background: transparent;
  border-radius: 50%;
  position: absolute;
  top: 0;
  left: 0;
}

.btn-login-zan-conetnt::after {
  border: none;
}

/*点赞图片样式*/

.img-like {
  margin-top: 20rpx;
  height: 70rpx;
  width: 64rpx;
  vertical-align: top;
  position: absolute;
  /*right: calc(50% - 25rpx);*/
  left: 50%;
  margin-left: -32rpx;
}

/*点赞个数样式*/

.view-like-count {
  color: #848484;
  font-size: 26rpx;
  padding-top: 92rpx;
}

/*评论个数，分割线，评论整体容器样式*/

.container-comments-count {
}

/*分隔线*/

.fengexian {
  margin-top: 40rpx;
  height: 16rpx;
  background: #f5f5f5;
}

/*评论数容器*/

.container-plcounts {
  font-size: 25rpx;
  color: #848484;
  padding: 26rpx;
}

/*评论分隔线*/

.plfengexian {
  height: 1rpx;
  background: #ededed;
}

/*评论item容器*/

.container-pl {
  position: relative;
  width: 100%;
  left: 0;
  top: 0;
}

/*评论头像*/

.pl-head {
  float: left;
  height: 64rpx;
  width: 64rpx;
  border-radius: 50%;
  margin-left: 26rpx;
  margin-top: 24rpx;
}

/*昵称，评论内容容器*/

.container-pl-nickname-content {
  float: left;
  margin-top: 26rpx;
  margin-left: 16rpx;
  width: calc(100vw - 200rpx);
}

/*昵称*/

.nickname {
  display: block;
  font-size: 24rpx;
  color: #727273;
}

/*评论内容*/

.plcontent {
  margin-top: 6rpx;
  font-size: 26rpx;
  display: block;
  color: #323232;
}

/*评论点赞容器*/

.container-pldianzan {
  position: absolute;
  right: 0;
  top: 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  padding-bottom: 10rpx;
  padding-left: 10rpx;
  overflow: visible;
  height: 90rpx;
  width: 110rpx;
}

.btn-login-zan-pl {
  position: relative;
  right: 0;
  top: 0;
  background: transparent;
  height: 90rpx;
  width: 110rpx;
}

.btn-login-zan-pl::after {
  border: none;
}

/*评论点赞文字*/

.text-plzan {
  color: #848484;
  font-size: 24rpx;
  position: absolute;
  right: 67rpx;
  top: 28rpx;
}

/*评论点赞图片*/

.img-plzan {
  top: 26rpx;
  height: 33rpx;
  width: 33rpx;
  position: absolute;
  right: 26rpx;
  background: white;
}

/*底部分割线*/

.bottom-fengexian {
  margin-top: 26rpx;
  height: 1rpx;
  background: #ededed;
  float: left;
  width: calc(100vw - 122rpx);
  margin-left: 96rpx;
  margin-right: 26rpx;
}

/*清楚浮动*/

.pl-clear-flex {
  clear: both;
}

/*底部评论输入框容器*/

.container-pl-input {
  position: fixed;
  width: 100%;
  bottom: 0;
  left: 0;
  overflow: visible;
  padding: 0;
}

/*输入框上方阴影*/

.image-yinying {
  width: 100vw;
  height: 30rpx;
  padding: 0;
  margin: 0;
  display: block;
}

/*输入框容器样式*/

.pl-input {
  height: 88rpx;
  background: white;
  float: left;
  width: 100vw;
  display: flex;
  flex-direction: row;
}

/*输入框样式*/

.input {
  height: 88rpx;
  font-size: 30rpx;
  flex: 1;
  color: #2d2d2d;
  padding-left: 26rpx;
}

.place-holder {
  color: #929292;
  font-size: 30rpx;
}

/*评论条数容器*/

.container-pl-count {
  height: 88rpx;
  width: 70rpx;
  display: inline-block;
  margin-left: 20rpx;
}

/*评论数量的图片*/

.img-pl-count {
  height: 32rpx;
  width: 36rpx;
  margin-top: 28rpx;
}

/*评论条数的文字*/

.text-pl-count {
  color: white;
  width: 40rpx;
  height: 26rpx;
  line-height: 26rpx;
  border-radius: 13rpx;
  background: red;
  margin-top: -40rpx;
  font-size: 20rpx;
  margin-left: -10rpx;
  display: inline-block;
  text-align: center;
  vertical-align: middle;
}

/*收藏容器*/

.container-collection {
  height: 88rpx;
  width: 88rpx;
  margin: 0 auto;
  overflow: visible;
  text-align: center;
  position: relative;
}

.collection-login-btn {
  height: 88rpx;
  width: 88rpx;
  margin: 0 auto;
  position: relative;
  background: transparent;
}

.collection-login-btn::after {
  border: none;
}

/*收藏的样式*/

.img-shoucang {
  height: 36rpx;
  width: 36rpx;
  margin-top: 26rpx;
  position: absolute;
  background: white;
}

/*emoji容器样式*/

.container-emoji {
  height: 88rpx;
  width: 88rpx;
}

/*emoji图片样式*/

.img-emoji {
  height: 60rpx;
  width: 60rpx;
  padding: 14rpx;
}

/*发送文字*/

.text-send {
  margin-left: 15rpx;
  margin-right: 16rpx;
  color: white;
  width: 120rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 8rpx;
  background: red;
  font-size: 35rpx;
  display: inline-block;
  text-align: center;
  vertical-align: middle;
  margin-top: 9rpx;
}

/*分享图片样式*/

.img-share {
  height: 88rpx;
  width: 158rpx;
  margin-top: -18rpx;
  padding: 0;
  background-size: 158rpx 118rpx;
  background-repeat: no-repeat;
  background-image: url("data:image/png;base64,");
}

button.img-share {
  border: none;
}

/*表情容器*/

.container-enmoji {
  margin-top: 88rpx;
  height: 200rpx;
  width: 100%;
  background: white
}

/*swiper的样式*/

.swiper-box {
  height: 200rpx;
}

.container-emoji {
  display: flex;
  flex-direction: row;
}

.container-emoji-item {
  align-items: center;
  display: unset;
  float: left;

}

